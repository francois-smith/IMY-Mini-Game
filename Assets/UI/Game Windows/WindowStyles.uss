    margin-bottom: 0;
    padding-left: 3px;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    overflow: visible;
}

.button #side-item {
    background-color: rgb(255, 255, 255);
    position: absolute;
    height: 60%;
