    left: -20px;
    align-items: auto;
    flex-grow: 1;
    justify-content: flex-end;
    visibility: hidden;
}

.button:hover #side-item {
    visibility: hidden;
}

.button:hover {
    color: rgb(169, 169, 169);
    background-color: rgba(0, 0, 0, 0);
